		<Element Type="SqlProcedure" Name="[dbo].[spIsUserDetailsValid]">
			<Property Name="BodyScript">
				<Value><![CDATA[
	BEGIN
		DECLARE @IsUserValid BIT
		SET @IsUserValid = 0

		IF EXISTS(SELECT * FROM tblUsers WHERE UserName = @UserName
										AND [Password] = @Password
										AND Roles = @Roles)
										
		BEGIN
			SET @IsUserValid = 1
		END

		SELECT @IsUserValid
	END]]></Value>
			</Property>
